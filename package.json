{
    "name": "colorful-regions",
    "displayName": "Colorful Regions",
    "description": "Colorize your code regions",
    "author": {
        "name": "Slobodan Radosavljevic"
    },
    "version": "1.0.0",
    "publisher": "hiduk",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:typescriptreact",
